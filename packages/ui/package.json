{
  "name": "@acme/ui",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    },
    "./*": {
      "default": [
        "./src/*.tsx",
        "./src/*.ts"
      ]
    },
    "./tiptap/*": {
      "default": [
        "./src/custom/tiptap/*.tsx",
        "./src/custom/tiptap/*.ts"
      ]
    },
    "./base-ui/*": {
      "default": [
        "./src/base-ui/*.tsx",
        "./src/base-ui/*.ts"
      ]
    },
    "./editor": {
      "default": [
        "./src/custom/editor/*.tsx",
        "./src/custom/editor/*.ts"
      ]
    },
    "./editor/plate": "./src/custom/editor/plate-editor.tsx",
    "./editor/simple": "./src/custom/simple-editor/index.tsx",
    "./lib/*": {
      "default": [
        "./src/lib/*.tsx",
        "./src/lib/*.ts"
      ]
    },
    "./hooks/*": {
      "default": [
        "./src/hooks/*.ts"
      ]
    },
    "./custom/*": {
      "default": [
        "./src/custom/*.tsx",
        "./src/custom/*.ts"
      ]
    }
  },
  "scripts": {
    "clean": "git clean -xdf node_modules .turbo",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint --flag unstable_native_nodejs_ts_config",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "ui:add": "pnpm dlx shadcn@latest add && prettier src --write --list-different",
    "ui-add": "pnpm dlx shadcn@latest add"
  },
