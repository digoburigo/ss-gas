{
  "name": "@acme/zen-v3",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*.ts",
    "./zenstack": "./src/zenstack/*.ts"
  },
  "scripts": {
    "check-types": "tsc --noEmit",
    "clean": "git clean -xdf node_modules .turbo .cache dist && rm -rf .cache",
    "db:down": "docker compose down",
    "db:generate": "zen generate -o ./src/zenstack",
    "db:migrate": "zen migrate dev",
    "db:reset": "zen migrate reset",
    "db:push": "pnpm with-env zen db push",
    "db:start": "docker compose up -d",
    "db:stop": "docker compose stop",
    "db:studio": "pnpm with-env drizzle-kit studio",
    "db:watch": "docker compose up",
    "dev": "tsc",
    "generate:component": "turbo gen react-component",
    "lint": "eslint . --max-warnings 0",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@zenstackhq/orm": "^3.1.1",
    "@zenstackhq/plugin-policy": "^3.1.1",
    "@zenstackhq/tanstack-query": "^3.1.1",
    "dotenv": "^17.2.3",
    "kysely": "catalog:",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@acme/tsconfig": "workspace:*",
    "@faker-js/faker": "^10.2.0",
    "@types/node": "catalog:",
    "@types/pg": "^8.16.0",
    "@zenstackhq/cli": "^3.1.1",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "prisma": "^7.2.0",
    "typescript": "catalog:"
  }
}
