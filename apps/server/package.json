{
  "name": "@acme/server",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*.ts"
  },
  "scripts": {
    "check-types": "tsc --noEmit",
    "clean": "git clean -xdf node_modules .turbo",
    "db:seed": "pnpm with-env tsx seed.ts",
    "dev": "pnpm with-env tsx watch src/index.ts",
    "build": "tsdown",
    "generate:component": "turbo gen react-component",
    "lint": "eslint . --max-warnings 0",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@acme/auth": "workspace:*",
    "@acme/zen-v3": "workspace:*",
    "@better-upload/server": "^3.0.2",
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/node": "^1.4.2",
    "@elysiajs/openapi": "^1.4.11",
    "@sinclair/typebox": "^0.34.41",
    "@zenstackhq/server": "^3.0.0-beta.20",
    "elysia": "^1.4.15",
    "kysely": "catalog:",
    "superjson": "2.2.5",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@acme/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "dotenv": "^17.2.3",
    "eslint": "catalog:",
    "tsdown": "^0.16.4",
    "tsx": "4.20.6",
    "typescript": "catalog:"
  }
}
